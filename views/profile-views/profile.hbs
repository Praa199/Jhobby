<div class="page-container" id="profile-container">
  <div>
    <h3>Hello:
      {{user.firstName}} {{user.lastName}}
    </h3>
    <h4>from {{user.location}}</h4>
    <img src="{{user.image}}" alt="image" srcset="">
    <a href="/profile/edit"><button class="btn btn-outline-primary">Update Profile</button></a>
  </div>
  <div class="own-posting">
    <div class="new-posting-button">
      <a href="/posting/new"><button class="btn btn-primary">New Post</button></a>
    </div>
    {{#each posting }}
    <div>
      <a href="/posting/{{this._id}}/view">
        <h3>
          {{this.title}}
        </h3>
      </a>
      <h2> â‚¬ {{this.hourlyFee}}</h2>
      <h3>{{this.date}}</h3>
    </div>
    <div>
      <p>
        description
        {{this.description}}
      </p>
    </div>
    <div>
      <img src="{{this.image}}" alt="">
    </div>
    <div>
      <a href="/posting/{{this._id}}/edit"><button class="btn btn-primary">Edit Post</button></a>
      <a href="/posting/{{this._id}}/delete"><button class="btn btn-outline-primary">Remove Post</button></a>
    </div>
    {{/each}}
  </div>
</div>